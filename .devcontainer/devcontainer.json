{
  "context": "..",
  "dockerFile": "./Dockerfile",
  "appPort": 3000,
  "extensions": ["dbaeumer.vscode-eslint", "esbenp.prettier-vscode"],
  "runArgs": ["--env-file", ".env"],
  "settings": {
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      {
        "language": "typescript",
        "autoFix": true
      },
      {
        "language": "typescriptreact",
        "autoFix": true
      }
    ]
  }
}
